#include<iostream>
#include<stack>
#include<queue>

using namespace std;

stack<int> Qsort(stack<int> s){
    stack<int> t;
    int temp;
    while(!s.empty()){
        temp = s.top();
        s.pop();
        while(!t.empty() && t.top() < temp){
            s.push(t.top());
            t.pop();
        }
        t.push(temp);
    }
    return t;
}


int main(){
    srand((unsigned)time(0));
    stack<int> s;
    for(int i=0; i<10; i++){
        s.push((rand())%100);
    }
    
    stack<int> t = Qsort(s);
    while(!t.empty()){
        cout<<t.top()<<endl;
        t.pop();
    }
}


