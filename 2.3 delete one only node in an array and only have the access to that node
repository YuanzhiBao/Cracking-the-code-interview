typedef struct node{
    int data;
    node *next;
}node;

node* intial(int a[], int n)
{
    node *head, *p;
    int i;
    for(i = 0; i<n; i++)
    {
        node *nd = new node();
        nd->data = a[i];
        if(i == 0)
        {
            head = p = nd;
            continue;
        }
        p->next = nd;
        p = nd;
        
    }
    return head;
}

void print(node *head)
{
    node *bitch = head;
    while(bitch)
    {
        cout<<bitch->data<<endl;
        bitch=bitch->next;
    }
}

bool dele(node *head, node *goal)
{
    if(goal == NULL || goal->next == NULL) return false;
    else
    {
        node *d = goal->next;
        goal->data = d->data;
        goal->next = d->next;
        delete d;//真不知道这句有啥用。
        return true;
    }
}

int main()
{
    int n = 5;
    int a[] = {2,3,10,9,4};
    node *head = intial(a, n);
    //print(head);
    node *goal = head;
    for(int i = 1; i<5; i++)
        goal= goal->next;
    if(dele(head, goal))
        print(head);
    else
        cout<<"this is a bug"<<endl;
    return 0;
}
