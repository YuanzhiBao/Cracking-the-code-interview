#include <iostream>
#include <cstdio>
using namespace std;

bool unique(string s)
{
    bool a[256];
    int len = s.length();
    for(int i = 0; i<len; i++)
    {
        int v = (int)(s[i]);
        if(a[v]) return false;
        a[v] = true;
    }
    return true;
}

int main()
{
    string s = "wertyuiolkjhgfdsw";//双引号表示字符串，单引号只是字符‘a’, "a" 都对。 "aafew"对, ‘fsaew’错
    cout<<unique(s)<<'\n'<<endl;
    return 0;
}
